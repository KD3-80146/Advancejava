package com.sunbeam.pojo;

public class Movie {
   
	private int id;
	private String title;
	private java.util.Date rel_date;

	public Movie() {

	}

	public Movie(int id, String title, Date rel_date) {
		this.id = id;
		this.title = title;
		this.rel_date = rel_date;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public java.util.Date getRel_date() {
		return rel_date;
	}

	public void setRel_date(java.util.Date rel_date) {
		this.rel_date = rel_date;
	}

	@Override
	public String toString() {
		return "Movie [id=" + id + ", title=" + title + ", rel_date=" + rel_date + "]";
	}
}
